{
    "name": "idcatch",
    "version": "1.0.0",
    "main": "index.js",
    "scripts": {
        "start": "expo start --dev-client",
        "android": "ENVFILE=.env react-native run-android --appIdSuffix=dev",
        "android-dev": "ENVFILE=.env.dev react-native run-android",
        "android-stage": "ENVFILE=.env.stage react-native run-android",
        "build-android-dev": "cd ./android && ENVFILE=.env.dev ./gradlew assembleDevRelease && cd ../",
        "build-android-stage": "cd ./android && ENVFILE=.env.stage ./gradlew assembleStageRelease && cd ../",
        "build-android-production": "cd ./android && ENVFILE=.env.production ./gradlew assembleRelease && cd ../",
        "ios": "expo run:ios",
        "web": "expo start --web"
    },
    "dependencies": {
        "@react-native-async-storage/async-storage": "^1.17.3",
        "@react-native-community/cameraroll": "^4.1.2",
        "@react-native-firebase/analytics": "^14.7.0",
        "@react-native-firebase/app": "^14.7.0",
        "@react-native-firebase/auth": "^14.7.0",
        "@react-native-firebase/firestore": "^14.7.0",
        "@react-native-firebase/storage": "^14.7.0",
        "@react-navigation/bottom-tabs": "^6.2.0",
        "@react-navigation/native": "^6.0.8",
        "@react-navigation/native-stack": "^6.5.0",
        "@reduxjs/toolkit": "^1.8.1",
        "expo": "~44.0.2",
        "expo-app-loading": "~1.3.0",
        "expo-local-authentication": "~12.1.0",
        "expo-splash-screen": "~0.14.1",
        "expo-status-bar": "~1.2.0",
        "firebase-admin": "^10.0.2",
        "iamport-react-native": "^2.0.0-rc.2",
        "prop-types": "^15.8.1",
        "react": "17.0.1",
        "react-dom": "17.0.1",
        "react-native": "0.64.3",
        "react-native-config": "^1.4.5",
        "react-native-gesture-handler": "^2.3.2",
        "react-native-image-auto-height": "^1.0.4",
        "react-native-rectangle-scanner": "^1.0.10",
        "react-native-redux": "^1.0.11",
        "react-native-responsive-screen": "^1.4.2",
        "react-native-restart": "^0.0.20",
        "react-native-safe-area-context": "3.3.2",
        "react-native-screens": "~3.10.1",
        "react-native-svg": "^12.3.0",
        "react-native-touch-id": "^4.4.1",
        "react-native-web": "0.17.1",
        "react-native-webview": "^11.18.1",
        "react-query": "^3.34.16",
        "react-redux": "^7.2.7",
        "redux-thunk": "^2.4.1",
        "styled-components": "^5.3.3"
    },
    "devDependencies": {
        "@babel/core": "^7.12.9",
        "yargs": "^17.3.1"
    },
    "private": true
}
